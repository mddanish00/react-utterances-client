{
  "name": "react-utterances-client",
  "version": "1.0.0",
  "description": "Another React component for using Utterances ðŸ”® on your website!",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:demo": "vite --config vite.democonfig.ts build",
    "size": "size-limit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "rimraf dist react-utterances-client-*",
    "format": "prettier {demo,src}/**/*.{js,jsx,ts,tsx,json} --write",
    "prepare": "npm run build"
  },
  "dependencies": {
    "classcat": "^5.0.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.2 || ^18.2.0",
    "react-dom": "^16.8.0 || ^17.0.2 || ^18.2.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "size-limit": "^11.0.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vite-plugin-dts": "^3.6.3"
  },
  "size-limit": [
    {
      "name": "cjs",
      "path": "dist/index.cjs"
    },
    {
      "name": "esm",
      "path": "dist/index.js"
    }
  ],
  "author": "mddanish00",
  "homepage": "https://mddanish00.github.io/react-utterances-client",
  "license": "MIT",
  "repository": "https://github.com/mddanish00/react-utterances-client",
  "keywords": [
    "utterances",
    "utterances component",
    "comments",
    "comments component",
    "react",
    "github issue"
  ]
}